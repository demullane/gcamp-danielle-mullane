      <div class = "page-header">
        <h1>Frequently Asked Questions</h1>
      </div>

      <% @questionscomb.each do |arr| %>
        <ul>
          <li><%= link_to arr.question, "#" + arr.slug %></li>
        </ul>
      <% end %>


      <% @questionscomb.each do |arr| %>
        <div class = "row">
          <div class = "col-md-6">
            <a id = "<%= arr.slug %>"></a>
            <strong><%= arr.question %></strong>
            <p><%= arr.answer %></p>
            <p style = "text-decoration: underline"><%= link_to "Top", "#top" %></p>
          </div>
        </div>
      <% end %>
