      <div class = "page-header">
        <h1>Frequently Asked Questions</h1>
      </div>

      <% @questionscomb.each do |arr| %>
        <ul>
          <li><%= link_to arr.question, "#arr.slug" %></li>
        </ul>
      <% end %>

      <% @questionscomb.each do |arr| %>
        <a id = "arr.slug"></a>
        <h3><%= arr.question %></h3>
        <p><%= arr.answer %></p>
        <p><%= link_to "Top", "#top" %></p>
      <% end %>
